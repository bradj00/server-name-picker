# phpIPAM service configuration
PORT=8003
NODE_ENV=development

# phpIPAM API connection
PHPIPAM_API_URL=https://ipam.example.com/api
PHPIPAM_API_APP_ID=your_app_id_here
PHPIPAM_API_USERNAME=your_username_here
PHPIPAM_API_PASSWORD=your_password_here
PHPIPAM_API_TOKEN=your_token_here
PHPIPAM_API_VERIFY_SSL=true

# Cache configuration
CACHE_TTL=300 # seconds

# Kafka configuration
KAFKA_BROKER=localhost:29092
KAFKA_CLIENT_ID=ipam-service
KAFKA_GROUP_ID=ipam-service-group
KAFKA_TOPIC_IP_REQUEST=ip-requests
KAFKA_TOPIC_IP_RESPONSE=ip-responses

# Logging
LOG_LEVEL=info